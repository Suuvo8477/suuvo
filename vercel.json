{
  "redirects": [
    {
      "source": "/.well-known/assetlinks.json",
      "has": [{ "type": "host", "value": "suuvo.com" }],
      "destination": "/.well-known/assetlinks.json",
      "statusCode": 200
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "suuvo.com" }],
      "destination": "https://www.suuvo.com/$1",
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/.well-known/assetlinks.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Content-Disposition",
          "value": "inline"
        }
      ]
    },
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
